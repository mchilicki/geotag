{
  "name": "geotag",
  "version": "1.0.0",
  "description": "Electron based desktop app for geotagging pictures with OpenStreetMap",
  "author": {
    "name": "Łukasz Błasiak, Marcin Chilicki, Wojciech Pszonak, Paweł Serwoński",
    "email": "wojciech.pszonak@gmail.com",
    "url": "https://github.com/mchilicki/geotag"
  },
  "build": {
    "appId": "pl.polsl.geotag",
    "files": [
      "**"
    ],
    "directories": {
      "buildResources": "."
    }
  },
  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "electron-run": "electron . true",
    "electron-build": "ng build --prod && npm run electron-run",
    "electron-dev": "concurrently --kill-others --success \"first\" \"ng serve\" \"electron . false\"",
    "complete-dist": "ng build --prod && electron-builder",
    "dist": "electron-builder"
  },
  "browser": {
    "fs": false,
    "path": false,
    "os": false
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.2.0",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/forms": "~7.2.0",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "core-js": "^2.5.4",
    "leaflet.markercluster": "^1.4.1",
    "ngx-cookie-service": "^2.1.0",
    "ngx-electron": "^2.1.1",
    "ngx-image-compress": "^7.2.2",
    "ol": "^5.3.3",
    "piexifjs": "^1.0.4",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "uikit": "^3.0.3",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular/cli": "~7.3.3",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "concurrently": "^4.1.0",
    "electron": "^7.2.4",
    "electron-builder": "^21.1.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.2.2"
  }
}
